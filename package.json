{
  "name": "reimagine-api",
  "version": "1.0.0",
  "main": "handler.js",
  "scripts": {
    "synthesizer": "ts-node synthesizer-test.ts",
    "segmentizer": "ts-node segmentizer-test.ts",
    "build": "rm -rf build && tsc && cp package.json ./build && cp package-lock.json ./build && cd build && npm i --production",
    "test": "jest --watchAll",
    "codegen": "graphql-codegen --config ./codegen.yml",
    "start": "ts-node src/api-lambda/run-locally.ts"
  },
  "dependencies": {
    "apollo-server-lambda": "^3.6.2",
    "aws-sdk": "^2.259.1",
    "graphql": "^16.3.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "md5": "^2.2.1",
    "midi-segmentizer": "0.0.6",
    "node-wav": "0.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.5.0",
    "@graphql-codegen/typescript": "^2.4.3",
    "@graphql-codegen/typescript-resolvers": "^2.5.0",
    "@types/jest": "^23.1.0",
    "@types/md5": "^2.1.32",
    "@types/node": "^8.0.57",
    "apollo-server": "^3.6.2",
    "jest": "^27.5.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "jest": {
    "setupFiles": [],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/.*\\.test.(ts|js)$"
  },
  "author": "Joseph Weidinger",
  "license": "MIT"
}
